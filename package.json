{
  "name": "n8n-nodes-tron",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Tron blockchain providing 5 resources and 14+ operations for TRX transfers, TRC-20 tokens, account management, and blockchain queries.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-tron.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-tron",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "tron",
    "trx",
    "blockchain",
    "cryptocurrency",
    "trc20",
    "web3",
    "trongrid"
  ],
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node esbuild.config.js && tsc --emitDeclarationOnly",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TronApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Tron/Tron.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "tronweb": "4.4.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
