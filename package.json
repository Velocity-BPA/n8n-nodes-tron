{
  "name": "n8n-nodes-tron",
  "version": "1.0.0",
  "description": "n8n community node for Tron",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/nodes/Tron/Tron.node.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.1.4",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "n8n-community-node-package",
    "tron",
    "blockchain",
    "cryptocurrency"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TronApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Tron/Tron.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "format": "prettier nodes credentials --write",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^2.0.0",
    "jest": "^29.0.0",
    "n8n-workflow": "*",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}